datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id          String  @id @default(cuid())
  title       String
  description String?
  price       Float   @default(100)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
