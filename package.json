{
  "name": "endrun",
  "description": "Endrun is a simple and powerful web API framework that dynamically generates endpoints and Swagger documentation based on database models using Prisma ORM.",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": {
    "email": "MooudMohammadi@gmail.com",
    "name": "Mooud Mohammadi tabar"
  },
  "version": "0.0.1",
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "dev": "nodemon src/server.ts",
    "lint": "tsc",
    "release": "npm run build && changeset publish",
    "postinstall": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MooudMohammady/endrun.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "typescript",
    "endrun",
    "web",
    "app",
    "http",
    "application",
    "framework",
    "router",
    "workers",
    "fastly",
    "nodejs"
  ],
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "prisma": "^5.9.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.17",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^3.12.2",
    "typescript": "^5.3.3"
  }
}
